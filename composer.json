        "meng-tian/async-soap-guzzle": "^0.2",
        "php": ">=6.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.5",
        "phpmd/phpmd": "@stable",
        "friendsofphp/php-cs-fixer": "^2.6"
    },
    "license": "MIT License",
    "authors": [{
        "name": "Darin Randal",
        "email": "darin.randal@gmail.com"
    }],
    "autoload": {
